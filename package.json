  "description": "Easy function of typescript",
  "repository": "https://github.com/guobin211/math-ts",
  "main": "dist/index.js",
  "scripts": {
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "tsc",
